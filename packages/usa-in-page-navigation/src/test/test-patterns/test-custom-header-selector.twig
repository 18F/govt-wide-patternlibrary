<div class="usa-in-page-nav-container">
  <aside class="usa-in-page-nav"
    {% if includeHeaders == "All" %}
      data-heading-selector="h1 h2 h3 h4 h5 h6"
      {% elseif includeHeaders == "Default" %}
      {% elseif includeHeaders == "h1" %}
        data-heading-selector="h1"
      {% elseif includeHeaders == "h2" %}
        data-heading-selector="h2"
      {% elseif includeHeaders == "h3" %}
        data-heading-selector="h3"
      {% elseif includeHeaders == "h4" %}
        data-heading-selector="h4"
      {% elseif includeHeaders == "h5" %}
        data-heading-selector="h5"
      {% elseif includeHeaders == "h6" %}
        data-heading-selector="h6"
      {% elseif includeHeaders == "Empty" %}
        data-heading-selector=""
      {% elseif includeHeaders == "Error" %}
        data-heading-selector="j1"
    {% endif %} >
  </aside>
  <main class="usa-prose">
    <h1>H1 heading</h1>
    <h2>H2 heading</h2>
    <h3>H3 heading</h3>
    <h4>H4 heading</h4>
    <h5>H5 heading</h5>
    <h6>H6 heading</h6>
  </main>
</div>
