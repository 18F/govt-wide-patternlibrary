{
  "name": "@uswds/uswds-core",
  "version": "0.0.1-alpha.0",
  "description": "Package to get USWDS core functionality, settings, tokens, functions, and mixins",
  "keywords": [
    "ui",
    "uswds",
    "sass",
    "open",
    "source",
    "united",
    "states",
    "usa",
    "u.s",
    "government"
  ],
  "author": "18F",
  "license": "SEE LICENSE IN LICENSE.MD",
  "exports": {
    "./sass": "./_index.scss",
    "./js/utils/*": "./src/js/utils/*.js",
    "./js/init/*": "./src/js/uswds-init.js",
    "./fonts/*": "./src/assets/fonts/*",
    "./favicons/*": "./src/img/favicons/*"
  },
  "scripts": {
    "test:js": "mocha './src/js/utils/test/*.spec.js'",
    "test:sass": "mocha './src/test/*.spec.js'",
    "test": "npm run test:js && npm run test:sass",
    "prepublishOnly": "npm run test"
  },
  "devDependencies": {
    "jsdom": "^20.0.0",
    "jsdom-global": "^3.0.2",
    "sass-true": "^6.0.1",
    "sinon": "^12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uswds/uswds.git"
  },
  "bugs": {
    "url": "https://github.com/uswds/uswds/issues"
  },
  "homepage": "https://github.com/uswds/src/stylesheets/core/README.md",
  "dependencies": {
    "classlist-polyfill": "^1.2.0",
    "domready": "^1.0.8",
    "object-assign": "^4.1.1",
    "receptor": "^1.0.0",
    "resolve-id-refs": "^0.1.0"
  }
}
