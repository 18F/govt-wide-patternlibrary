<form class="usa-form">
  <label class="usa-label" for="options">Select</label>
  <select class="usa-select" name="options" id="options">
    <option value>- Select -</option>
    <option value="value1">Option A</option>
    <option value="value2">Option B</option>
    <option value="value3">Option C</option>
  </select>
</form>

<form class="usa-form">
  <label class="usa-label" for="disabled-options">Disabled select</label>
  <select class="usa-select" name="disabled-options" id="disabled-options" disabled>
    <option value>- Select -</option>
    <option value="value1">Option A</option>
    <option value="value2">Option B</option>
    <option value="value3">Option C</option>
  </select>
</form>

<form class="usa-form">
  <label class="usa-label" for="default-options">Disabled select - browser default <br/>(no .usa-select)</label>
  <select name="default-options" id="default-options" disabled>
    <option value>- Select -</option>
    <option value="value1">Option A</option>
    <option value="value2">Option B</option>
    <option value="value3">Option C</option>
  </select>
</form>

<label class="usa-label" for="input-type-text">Input</label>
<input class="usa-input" id="input-type-text" name="input-type-text" type="text" >

<label class="usa-label" for="disabled-input-type-text">Disabled input</label>
<input class="usa-input" id="disabled-input-type-text" name="disabled-input-type-text" type="text" disabled>


<label class="usa-label" for="default-input-type-text">browser default <br/>(no .usa-input)</label>
<input id="default-input-type-text" name="default-input-type-text" type="text" disabled>
